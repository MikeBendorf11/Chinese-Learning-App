{
  "version": "0.1.0",
  "configurations": [
      {
          "name": "Launch localhost",
          "type": "chrome",
          "request": "launch",
          "url": "http://localhost:8080/index.html",
          "webRoot": "${workspaceFolder}/static",
          "skipFiles": [
            "${workspaceFolder}/static/bootstrap.js",
            "${workspaceFolder}/static/jquery.min.js",
            "${workspaceFolder}/static/popper.min.js",
          ]
      },
      {
          "name": "Launch index.html",
          "type": "chrome",
          "request": "launch",
          "file": "${workspaceFolder}/index.html"
      },
  ]
}